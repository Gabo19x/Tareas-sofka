{{>head titulo="Inicio"}}


<body>
    <h1>
        {{titulo}} || {{nombre}}
    </h1>

    {{#if mayor}}
        <p> Eres mayor de edad {{edad}}</p>
    {{else}}
        <p> Eres menor de edad {{edad}}</p>
    {{/if}}

    <input type="text" placeholder="Nombre" name="nombre">
    <input type="text" placeholder="ID" name="id">
    <button type="submit">Agregar</button>

</body>
</html>

